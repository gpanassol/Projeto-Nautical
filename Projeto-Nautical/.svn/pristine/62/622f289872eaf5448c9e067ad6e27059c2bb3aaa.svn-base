using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Projeto
{
    class Usuario
    {
        private string _matricula;
        private string _nome;
        private string _endereco;
        private string _telefone;
        private string _rg;
        private string _cpf;
        private string _sexo;
        private string _login;
        private string _senha;
        private string _permissao;

        public string Nome
        {
            get { return _nome; }
            set { _nome = value; }
        }


        public string Endereco
        {
            get { return _endereco; }
            set { _endereco = value; }
        }


        public string Telefone
        {
            get { return _telefone; }
            set { _telefone = value; }
        }


        public string Rg
        {
            get { return _rg; }
            set { _rg = value; }
        }


        public string Cpf
        {
            get { return _cpf; }
            set { _cpf = value; }
        }


        public string Sexo
        {
            get { return _sexo; }
            set { _sexo = value; }
        }


        public string Login
        {
            get { return _login; }
            set { _login = value; }
        }


        public string Senha
        {
            get { return _senha; }
            set { _senha = value; }
        }


        public string Permissao
        {
            get { return _permissao; }
            set { _permissao = value; }
        }


        public string Matricula
        {
            get { return _matricula; }
            set { _matricula = value; }
        }
    }
}
